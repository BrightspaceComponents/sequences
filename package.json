{
  "name": "d2l-sequences",
  "description": "",
  "private": true,
  "scripts": {
    "build:lang": "gulp build-locales",
    "build": "npm run build:lang",
    "postinstall": "polymer install",
    "lint:fix": "eslint *.html demo/*.html mixins/*.html test/*.html --fix",
    "lint:html": "eslint *.html demo/*.html mixins/*.html test/*.html",
    "lint": "npm run lint:html && polymer lint --input ./{components,mixins,test}/*.html",
    "pretest": "npm run lint",
    "test": "polymer test --skip-plugin sauce",
    "test-p": "polymer test --skip-plugin sauce -p",
    "locales": "gulp build-locales && npm run gen:localize",
    "gen:localize": "plop localize",
    "start": "polymer analyze > analysis.json && polymer serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceComponents/sequences"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai-dom": "^1.8.0",
    "eslint": "^4.2.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-merge-json": "^1.3.1",
    "plop": "^2.0.0",
    "polymer-cli": "^1.7.2",
    "polymer-siren-test-helpers": "^1.0.0"
  }
}
